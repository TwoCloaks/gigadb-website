add_formatter :min
checksum_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/checksums'
cookbook_path [
               '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/chef-cookbooks',
               '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/site-cookbooks'
              ]
data_bag_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/data_bags'
environment 'production'
environment_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/environments'
file_backup_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/backup'
file_cache_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/cache'
json_attribs nil
lockfile '<%= node[:gigadb][:site_dir] %>/chef.pid'
log_level :auto
log_location STDOUT
node_name 'mynode.example.com'
rest_timeout 300
role_path '<%= node[:gigadb][:site_dir] %>/gigadb-website/chef/roles'
sandbox_path '/tmp/cookbooks'
solo false
syntax_check_cache_path
umask 0022
verbose_logging true